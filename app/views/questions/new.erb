<h1>Submit a new question!</h1>
<%if @errors %>
  <%@errors.each do |error|%>
  <h4 style="color: red"><%=error%></h4>
  <%end%>
<%end%>
<form method="Post" action="/questions" align="left">
  Title:
  <input type="text" name="question[title]"><br><br>
 Body:
  <input type="text" name="question[body]"><br><br>
  <input type="submit" value="Submit">
</form>